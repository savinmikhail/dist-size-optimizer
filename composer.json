{
    "name": "savinmikhail/export-ignore-check",
    "description": "Checks that you have all non-production files excluded from dist",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.2",
        "symfony/finder": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^10.5",
        "phpyh/coding-standard": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "SavinMikhail\\ExportIgnore\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SavinMikhail\\ExportIgnore\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/export-ignore"
    ]
}
